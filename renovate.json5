{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "helpers:pinGitHubActionDigests",
  ],
  "additionalReviewers": ["sksat"],
  "regexManagers": [
    "fileMatch": ["^action.yaml"],
    "matchStrings": [
      "depName=(?<depName>.*?) datasource=(?<datasource>.*?)\\n\\s+default: \\'(?<currentValue>.*?)\\'",
    ],
  ],
}
